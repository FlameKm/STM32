

&led0 {
    gpios = <&gpio1 6 GPIO_ACTIVE_HIGH>;
    label = "Red LED 0";
};
&led1 {
    gpios = <&gpio1 7 GPIO_ACTIVE_HIGH>;
    label = "Red LED 1";
};
&button0 {
    gpios = <&gpio1 8 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
    label = "Push button 1";
};
&button1 {
    gpios = <&gpio1 9 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
    label = "Push button 2";
};
